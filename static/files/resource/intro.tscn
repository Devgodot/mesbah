uniform float scale : hint_range(0.01, 10.0, 0.01) = 1.0;
void vertex() {

}

void fragment() {
	vec2 uv = UV / scale;
    uv = fract(uv); // wrap into 0-1 range
    uv = 1.0 - abs(1.0 - 2.0 * uv); // mirror repeat
	COLOR = texture(tex, uv);
	COLOR.rgb *= vec3(brigth);

}

