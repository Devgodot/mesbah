[sub_resource type="GDScript" id="GDScript_apcft"]
script/source = "extends PanelContainer
signal screen_entered
signal response
signal edit
var has_on_screen = false
@export var dir = 1
var move = false
var start_pos = 0
var seen
var parent:Control
var pre_node 
var next_node
var pre_size = 0
var next_size = 0
var label
var beep = false
var index 
var checked = false:
	set(x):
		if checked == false:
			if label:
				label.modulate.a = 1
			modulate.a = 1
			pivot_offset.x = size.x / 2
			var tween = get_tree().create_tween()
			
			tween.tween_property(self, \"scale\", Vector2.ONE, 0.4)
			tween.play()
			checked = x
		
func _ready() -> void:
	if get_parent().get_parent() is Control:
		parent = get_parent().get_parent()
func _input(event: InputEvent) -> void:
	if visible and parent and Updatedate.bg.visible == false:
		if event is InputEventMouseButton:
			if event.button_index == MOUSE_BUTTON_RIGHT and event.is_pressed():
				if event.position.y < parent.global_position.y + parent.size.y:
					var rect = Rect2(Vector2(0, global_position.y), Vector2(get_parent().size.x, size.y))
					if rect.has_point(event.position) :
						edit.emit(event.position.y)
		if event is InputEventScreenDrag:
			if event.position.y < parent.global_position.y + parent.size.y:
				var rect = Rect2(Vector2(0, global_position.y), Vector2(get_parent().size.x, size.y))
				if rect.has_point(event.position) and move == false and parent.move == false:
					var angle = rad_to_deg(event.relative.angle())
					if dir == 1:
						if (angle < 2 and angle > -2) and abs(event.relative.x) > 5:
							move = true
							parent.move = true
							start_pos = event.position.x
							
					if dir == -1:
						if (angle > 178 and angle < 182) and abs(event.relative.x) > 5:
							move = true
							parent.move = true
							start_pos = event.position.x
		if event is InputEventMouseButton:
			if event.is_released():
				if move :
					if dir == 1:
						if global_position.x > (parent.size.x - size.x) + (size.x / 2.0) - 30:
							response.emit()
						var tween = get_tree().create_tween()
						tween.tween_property(self, \"position:x\", (get_parent().size.x - size.x), 0.2)
						tween.play()
						await tween.finished
					if dir == -1:
						if global_position.x < (-size.x / 2.0) + 30:
							response.emit()
						var tween = get_tree().create_tween()
						tween.tween_property(self, \"position:x\", 0, 0.2)
						tween.play()
						await tween.finished
				move = false
				parent.move = false

func _process(delta: float) -> void:
	size = get_combined_minimum_size()
	if move:
		if dir == 1:
			if get_parent().get_parent() is Control:
				position.x = (get_parent().size.x - size.x) + (get_global_mouse_position().x - start_pos)
				if global_position.x > (parent.size.x - size.x) + (size.x / 2.0) - 30:
					if beep == false:
					
						beep = true
						DisplayServer.beep()
				else:
					beep = false
				position.x = clamp(position.x, get_parent().size.x - size.x, (get_parent().size.x - size.x) + (size.x / 2))
		if dir == -1:
			position.x = (get_global_mouse_position().x - start_pos)
			if global_position.x < (-size.x / 2.0) + 30:
				if beep == false:
					beep = true

					DisplayServer.beep()
			else:
				beep = false
			position.x = clamp(position.x, - (size.x / 2), 0)
	else:
		beep = false
		if dir == 1:
			position.x = get_parent().size.x - size.x
		else:
			position.x = 0
	if get_parent().get_parent() is Control:
		if has_on_viewport(get_parent().get_parent()):
			pre_node = get_parent().get_child(get_index() - 1) if get_index() > 3 else null
			if checked and !has_on_screen:
				screen_entered.emit(seen)
				has_on_screen = true
		else:
			out_off_range()
			has_on_screen = false
	if next_node:
		next_size = next_node.size.y
	
	if index == null:
		if pre_node and pre_node.index != null:
			index = pre_node.index + 1 
		else:
			if next_node and next_node.index != null:
				index = next_node.index - 1
			else:
				if has_on_screen == false:
					global_position.y = $\"../..\".global_position.y + 73
				else:
					index = $\"../../../..\".last_index
	
	if index != 0 and pre_node == null and next_node == null:
		var ext = 0
		if label:
			ext = 73
		global_position.y = $\"../..\".global_position.y + ext
		
	if pre_node:
		if label:
			global_position.y = pre_node.global_position.y + pre_node.size.y + 10 + label.size.y
		else:
			global_position.y = pre_node.global_position.y + pre_node.size.y + 10
		
		pre_size = pre_node.size.y
		checked = true
	elif next_node:
		if checked == false:
			global_position.y = next_node.global_position.y - size.y - 10 if next_node.label == null else next_node.global_position.y - size.y - 73
			next_node.pre_node = self
			checked = true
	if label:
		if label.is_ready:
			label.size.x = parent.size.x
		label.global_position.y = global_position.y - 10 - label.size.y
	if next_node == null :
		if has_on_viewport(get_parent().get_parent()) and index and index < get_parent().get_parent().get_parent().get_parent().max_message - 1:
			screen_entered.emit(seen)
	if pre_node == null :
		if has_on_viewport(get_parent().get_parent()) and index and index > 0 and get_parent().get_parent().get_parent().get_parent().max_message > 1:
			screen_entered.emit(seen)
			
func has_on_viewport(parent:Control) -> bool:
	var extera_size = 0
	if label:
		extera_size += label.size.y
	if global_position.y - extera_size >= parent.global_position.y and global_position.y <= parent.global_position.y + parent.size.y - size.y:
		return true
	return false
func out_off_range():
	if visible and checked:
		var ext = 0
		if label:
			ext = 73
		if global_position.y < parent.global_position.y - (size.y + 20 + ext) :
			queue_free()
			if label:
				label.queue_free()
			if next_node:
				next_node.pre_node = null
		if global_position.y > parent.global_position.y + parent.size.y + ext:
			queue_free()
			if label:
				label.queue_free()
			if pre_node:
				pre_node.next_node = null
"

