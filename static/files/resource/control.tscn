[sub_resource type="GDScript" id="GDScript_vb5ih"]
script/source = "extends Control

signal scroll_start
signal scroll_ended
var max_scroll = 1
var separation = 30
var drag = true
var scroll = Vector2.ZERO
var offset = 0
var begin_id = \"\"
var last_id = \"\"
var can_drag = [true, true]
var last_height = 0
var start = false
var move = false
var dir = 1
var scroll_offset = 0
var velocity = 0.0
var dragging = false
var last_mouse_pos = Vector2.ZERO

func _notification(what: int) -> void:
	if what == NOTIFICATION_RESIZED:
		var box = $VBoxContainer.get_child(-1)
		if $\"../..\".edited_box:
			box =  $\"../..\".edited_box
		if last_height != 0 and box.global_position.y + box.size.y >= size.y:
			$VBoxContainer.position.y -= last_height - size.y
			scroll_offset -= last_height - size.y
		last_height = size.y
func _ready() -> void:
	last_height = size.y
	
# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	
	can_drag[0] = (not get_tree().has_group(begin_id)) or (get_tree().has_group(begin_id) and get_tree().get_first_node_in_group(begin_id).global_position.y - 63 < global_position.y)
	if last_id != \"\":
		if get_tree().has_group(last_id):
			var btn = get_tree().get_first_node_in_group(last_id)
			if int(btn.global_position.y) <= int(global_position.y) + size.y - btn.size.y:
				can_drag[1] = false
			else:
				can_drag[1] = true
		else:
			can_drag[1] = true
	else: 
		can_drag[1] = false
	if not dragging:
		if abs(velocity) > 4:
			drag = true
			scroll_offset += velocity * delta
			velocity *= pow(0.92, 60*delta)
		else:
			velocity = 0.0
	_apply_scroll()
	#if scroll.y < 0:
		#dir = -1
	#else :
		#dir = 1
	#if int(scroll.y) != 0:
		##if abs(scroll.y) > 2000:
			##scroll.y = 2000 * dir
		#scroll.y *= 0.95
		#if abs(scroll.y) < 5:
			#scroll.y = 0
		#$VBoxContainer.position += scroll * delta
func _apply_scroll():
	if get_tree().has_group(begin_id):
		var btn = get_tree().get_first_node_in_group(begin_id)
		if btn.checked:
			if int(scroll_offset) > -btn.position.y + 73:
				$VBoxContainer.position.y = -btn.position.y + 73
				scroll_offset =  -btn.position.y + 73
				velocity = 0.0
				
			if $VBoxContainer.position.y == -btn.position.y + 73:
				scroll.y = 0
	if get_tree().has_group(last_id) and can_drag[0]:
		var btn = get_tree().get_first_node_in_group(last_id)
		if btn.checked:
			var _delta = (size.y) - (btn.size.y)
			if int(scroll_offset) < int(_delta - btn.position.y):
				scroll_ended.emit()
				$VBoxContainer.position.y =  _delta-btn.position.y
				scroll_offset = _delta-btn.position.y
				velocity = 0.0
			if $VBoxContainer.position.y == _delta-btn.position.y:
				scroll.y = 0
	
	$VBoxContainer.position.y = scroll_offset
func _gui_input(event: InputEvent) -> void:
	if move == false:
		#if event is InputEventScreenDrag:
			#if event.relative.y > 1 and can_drag[0]:
				#prints(event.relative, event.velocity)
				#scroll += (Vector2(0, event.relative.y*5))
				#scroll_start.emit(1)
				#dir = 1
			#if event.relative.y < -1 and can_drag[1]:
				#
				#scroll = (Vector2(0, event.velocity.y))
				#
				#scroll_start.emit(-1)
				#dir = -1
		if event is InputEventMouseButton:
			if event.button_index == MOUSE_BUTTON_LEFT:
				if event.pressed:
					dragging = true
					velocity = 0.0
					last_mouse_pos = event.position
				else:
					dragging = false
			if event.is_pressed() and event.button_index == MOUSE_BUTTON_WHEEL_DOWN:
				if can_drag[1]:
					dir = -1
					scroll = (Vector2(0, -4000 * event.factor))
					scroll_start.emit(-1)
			if event.is_pressed() and event.button_index == MOUSE_BUTTON_WHEEL_UP:
				if can_drag[0]:
					dir = 1
					scroll = (Vector2(0, 4000 * event.factor))
					scroll_start.emit(1)
		elif event is InputEventMouseMotion and dragging:
			var delta = event.position - last_mouse_pos
			if delta.y > 0 :
				if can_drag[0] == false:
					return
			else:
				if can_drag[1] == false:
					return
			if abs(delta.y) > 1:
				scroll_start.emit(1 if delta.y > 0 else -1)
				scroll_offset += delta.y
				velocity = lerp(velocity ,delta.y / get_process_delta_time(), 0.4)
			last_mouse_pos = event.position
"

