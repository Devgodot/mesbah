script/source = "extends Label

var is_ready = false
var _text = \"\"
var box :Control
# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	position.x = (get_parent().size.x / 2) - (size.x / 2)
	var tween = get_tree().create_tween()
	tween.tween_property(self, \"position:x\", 0, 0.4)
	tween.play()
	var tween2 = get_tree().create_tween()
	tween2.tween_property(self, \"size:x\", get_parent().size.x, 0.4)
	tween2.play()
	await tween.finished
	var tween3 = get_tree().create_tween()
	tween3.tween_property(self, \"text\", _text, 0.2)
	tween3.play()
	is_ready = true
func _process(delta: float) -> void:
	if is_ready and visible:
		if box == null or box.is_queued_for_deletion():
			queue_free()
"

