script/source = "extends Control
signal start_scroll
var max_scroll = 1
var separation = 30
var drag = false
var _scroll = Vector2.ZERO
var move = false
var run_tween = false
var height = 0

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	if height <= 0:
		height = size.y -10
	if run_tween:
		scroll(Vector2.ZERO)
	manage_scroll($VBoxContainer)

func get_item_pos(id:String, dir=1):
	if move == false:
		var item = get_tree().get_nodes_in_group(\"message\").filter(func (x): return x.get_meta(\"id\", \"\") == id)
		if item.size() == 0:
			while item.size() == 0:
				scroll(Vector2(0, -100*dir))
				move = true
				item = get_tree().get_nodes_in_group(\"message\").filter(func (x): return x.get_meta(\"id\", \"\") == id)
				await get_tree().physics_frame
		var tween = get_tree().create_tween()
		tween.tween_property(self, \"_scroll:y\", item[0].position.y if dir == 1 else item[0].position.y - item[0].size.y, 0.2)
		tween.play()
		move = true
		run_tween = true
		await tween.finished
		run_tween = false
		move = false
		scroll(Vector2.ZERO)
		return item[0]
func set_child_size():
	var pos = 0
	for child in get_children():
		if child is Control:
			child.position.y = pos
			child.set_meta(\"pos\",child.position.y)
			var _size = child.size.y
			for subchild in child.get_children():
				if subchild is Control and \"instance\" not in subchild.name:
					subchild.position.y = _size
					_size += subchild.size.y + separation
				
			pos += _size
			
			child.set_deferred(\"size:y\", _size)
	max_scroll = pos
	scroll(Vector2.ZERO)

func set_input_child(process):
	for child in get_children():
		for subchild in child.get_children():
			if subchild is Control:
				subchild.set_process_input(process)
func _gui_input(event: InputEvent) -> void:
	if move == false:
		if event is InputEventScreenDrag:
			scroll(Vector2(0, -event.relative.y))
			drag = true
		if event is InputEventMouseButton:
			if event.is_pressed() and event.button_index == MOUSE_BUTTON_WHEEL_DOWN:
				scroll(Vector2(0, 100 * event.factor))
			if event.is_pressed() and event.button_index == MOUSE_BUTTON_WHEEL_UP:
				scroll(Vector2(0, -100 * event.factor))
func scroll(velloity:Vector2):
	if velloity.y > 0 and !move and _scroll.y <= max_scroll-size.y - 100:
		start_scroll.emit()
	$\"../../ColorRect2\".hide()
	_scroll += velloity
	_scroll.y = clamp(_scroll.y, -abs(height - size.y), max_scroll-size.y)
	for child in get_children():
		child.position.y = -_scroll.y + child.get_meta(\"pos\", 0) - (height - size.y)
		child.position.y = clamp(child.position.y, -max_scroll+size.y+child.get_meta(\"pos\", 0), child.get_meta(\"pos\", 0))
func _notification(what: int) -> void:
	if what == NOTIFICATION_RESIZED:
		scroll(Vector2.ZERO)
func manage_scroll(box):
	for child in box.get_children():
		if \"instance\" not in child.name:
			var space = separation
			var top_pos = global_position.y - child.size.y - space
			var bottom_pos = global_position.y + size.y + child.size.y + space
			var left_pos = global_position.x - child.size.x - space
			var right_pos = global_position.x + size.x + space
			var child_pos = child.global_position
			if child_pos.y < top_pos or child_pos.y > bottom_pos or child_pos.x < left_pos or child_pos.x > right_pos:
				child.hide()
			else:
				child.show()
"

[sub_resource type="GDScript" id="GDScript_2pn8f"]
script/source = "@tool
extends Container

@export var separation: int = 8

func _notification(what):
	if what == NOTIFICATION_SORT_CHILDREN:
		sort_children_custom()
func sort_children_custom():
	var y := 0.0
	var container_width := size.x
	for child in get_children():
		if not child is Control or \"instance\" in child.name:
			continue

		var min_size = child.get_combined_minimum_size()
		var desired_width = min_size.x
		var desired_height = min_size.y

		# اگر FILL باشه، کل عرض رو بگیر
		if child.size_flags_horizontal & Control.SIZE_FILL != 0:
			desired_width = container_width

		# محاسبه موقعیت X بر اساس SHRINK_MODE
		var x := 0.0

		var shrink_mode = child.size_flags_horizontal
		match shrink_mode:
			Control.SIZE_SHRINK_BEGIN:
				x = 0
			Control.SIZE_SHRINK_CENTER:
				x = (container_width - desired_width) / 2
			Control.SIZE_SHRINK_END:
				x = container_width - desired_width
			_:
				# حالت پیش‌فرض
				x = 0

		# اعمال اندازه و موقعیت
		child.position = Vector2(x, y)
		child.size = Vector2(desired_width, desired_height)
		y += desired_height + separation
	size.y = y
	if get_parent().get(\"max_scroll\"):
		get_parent().max_scroll = y
"

