script/source = "extends Button
signal screen_entered
var has_on_screen = false
var parent:Control
var next_node
var pre_node
var count = 5
var _index = null
var label
func _ready() -> void:
	if get_parent().get_parent().get_parent() is Control:
		parent = get_parent().get_parent().get_parent()
	Updatedate.recive_message.connect(func(message, id):
		if _index == 0 and message.conversationId + message.part != get_meta(\"id\", \"\"):
			_index = 1
			position.y += 210)
func _process(delta: float) -> void:
	if parent and name != \"instance\":
		size.x = parent.size.x
		if pre_node:
			if label:
				position.y = pre_node.position.y + 420
			else :
				position.y = pre_node.position.y + 210
				
			_index = pre_node._index + 1 if pre_node._index != null else null
		else:
			if next_node and _index == null and next_node._index != null:
				_index = next_node._index - 1
				#if next_node._index == null:
					#_index = $\"../../../../..\".last_c
				#else:
					#
			#else:
				#_index = $\"../../../../..\".last_c
		if has_on_viewport(parent):
			if has_on_screen == false:
				screen_entered.emit()
				has_on_screen = true
		else:
			has_on_screen = false
			out_off_range()
func has_on_viewport(parent:Control) -> bool:
	if global_position.y >= parent.global_position.y and global_position.y <= parent.global_position.y + parent.size.y - size.y:
		return true
	return false
func out_off_range():
	if global_position.y < parent.global_position.y - (210 * count) :
		queue_free()
		if label:
			label.queue_free()
		if next_node:
			next_node.pre_node = null
	if global_position.y > parent.global_position.y + parent.size.y + (210 * count):
		queue_free()
		if label:
			label.queue_free()
		if pre_node:
			pre_node.next_node = null
"

