[sub_resource type="Shader" id="Shader_6hmx6"]
code = "shader_type canvas_item;
uniform sampler2D pattern : repeat_enable;
uniform float scale=1.0;
uniform vec4 back_color:source_color;
uniform vec4 fore_color:source_color;
void vertex() {
	// Called for every vertex the material is visible on.
}

void fragment() {
	vec4 color = texture(pattern, UV * scale);
	if (color.r >= 0.9 && color.g >= 0.9 && color.b >= 0.9){
		color.rgb = back_color.rgb;
	}else{
		color.rgb = fore_color.rgb ;
	}
	vec4 tex = texture(TEXTURE, UV);
	COLOR = color;
	COLOR.a = tex.a;
}

//void light() {
