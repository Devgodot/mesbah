[sub_resource type="GDScript" id="GDScript_cc30s"]
script/source = "extends Control

signal scroll_start(offset_y:int, max:int)
var max_scroll = 1
var separation = 30
var drag = false
var scroll = Vector2.ZERO
var offset = 0
var begin_id = \"\"
var last_id = \"\"
var can_drag = [false, true]
# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	scroll = lerp(scroll, Vector2.ZERO, 0.5)
	can_drag[0] = (not get_tree().has_group(begin_id)) or(get_tree().has_group(begin_id) and get_tree().get_first_node_in_group(begin_id).global_position.y < global_position.y)
	if last_id != \"\":
		if get_tree().has_group(last_id):
			var btn = get_tree().get_first_node_in_group(last_id)
			if btn.global_position.y < global_position.y + size.y - btn.size.y:
				can_drag[1] = false
				if can_drag[0]:
					print(0)
					$VBoxContainer.get_child(1).position.y += (global_position.y + size.y - btn.size.y) - btn.global_position.y
			else:
				can_drag[1] = true
		else:
			can_drag[1] = true
	else: 
		can_drag[1] = false
	if get_tree().has_group(begin_id):
		if get_tree().get_first_node_in_group(begin_id).global_position.y > global_position.y:
			var tween = get_tree().create_tween()
			tween.tween_property($VBoxContainer, \"position:y\", 0, 0.1)
			tween.play()
			var tween2 = get_tree().create_tween()
			tween2.tween_property(get_tree().get_first_node_in_group(begin_id), \"position:y\", 0, 0.1)
			tween2.play()
	if scroll != Vector2.ZERO:
		$\"../Control/VSlider\".value -= scroll.y
		$VBoxContainer.position += scroll
		$VBoxContainer.position.y = clamp($VBoxContainer.position.y, -211, 210)
		if $VBoxContainer.get_child_count() > 1:
			if $VBoxContainer.position.y < -210:
				$VBoxContainer.get_child(1).position.y -= 210
				$VBoxContainer.position.y = 0
			if $VBoxContainer.position.y >= 210:
				$VBoxContainer.get_child(1).position.y += 210
				$VBoxContainer.position.y = 0
func set_child_offset(offset):
	for child in get_children():
		child.position.y = offset + child.get_meta(\"pos\", 0)
		child.position.y = clamp(child.position.y, -max_scroll+size.y + child.get_meta(\"pos\", 0), child.get_meta(\"pos\", 0))
func set_child_size():
	var pos = 0
	for child in get_children():
		if child is Control:
			child.position.y = pos 
			child.set_meta(\"pos\",pos)
			var _size = child.size.y
			for subchild in child.get_children():
				if subchild is Control and subchild.name != \"instance\":
					subchild.position.y = _size
					_size += subchild.size.y + separation
			pos += _size
			
			child.set_deferred(\"size:y\", _size)
	max_scroll = pos
func set_input_child(process):
	for child in get_children():
		for subchild in child.get_children():
			if subchild is Control:
				subchild.set_process_input(process)
func _gui_input(event: InputEvent) -> void:
	if event is InputEventScreenDrag:
		if event.relative.y > 4 and can_drag[0]:
			scroll = (Vector2(0, event.relative.y))
			drag = true
		if event.relative.y < -4 and can_drag[1]:
			scroll = (Vector2(0, event.relative.y))
			drag = true
	if event is InputEventMouseButton:
		if event.is_pressed() and event.button_index == MOUSE_BUTTON_WHEEL_DOWN:
			if can_drag[1]:
				scroll = (Vector2(0, -100 * event.factor))
		if event.is_pressed() and event.button_index == MOUSE_BUTTON_WHEEL_UP:
			if can_drag[0]:
				scroll = (Vector2(0, 100 * event.factor))
